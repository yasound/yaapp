# Where the Django project is.
CELERYBEAT_CHDIR="/var/www/dev.yasound.com/root/yaapp"

# Name of the projects settings module.
export DJANGO_SETTINGS_MODULE="deploy"

# Path to celerybeat
CELERYBEAT="$CELERYBEAT_CHDIR/manage.py celerybeat"

# Workers should run as an unprivileged user.
CELERYBEAT_USER="customer"
CELERYBEAT_GROUP="customer"



# Extra arguments to celerybeat
CELERYBEAT_OPTS="--schedule=/home/customer/db/celery/celerybeat-schedule"

ENV='development'
